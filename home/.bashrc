# General
alias ..='cd ..'
alias ll='ls -Glh'
alias bashrc='vim ~/.bashrc'
alias vimrc='vim ~/.vimrc'
alias ta='tmux a'

# Git
alias ga='git add'
alias gb='git branch'
alias gc='git checkout'
alias gd='git diff --color --no-prefix'
alias gdc='git diff --cached --no-prefix'
alias gg='git status'
alias gl='git log'
alias gp='git pull'
alias gsh='git show'
alias gst='git stash'
alias gstp='git stash pop'
alias gcml='git checkout mainline'
alias mainline='git checkout mainline'

# PATH
export PATH=~/bin:$PATH
